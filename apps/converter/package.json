{
  "name": "@escp/converter",
  "version": "1.0.0",
  "description": "REST API for converting ESC/P2 PRN files to PDF/PNG",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "clean": "rm -rf dist .turbo"
  },
  "keywords": [
    "escp",
    "escp2",
    "converter",
    "prn",
    "pdf",
    "png",
    "api"
  ],
  "author": "Mohammad AL Mechkor",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-zod": "^3.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@escp/config": "workspace:*",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.1",
    "supertest": "^7.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18"
  }
}
